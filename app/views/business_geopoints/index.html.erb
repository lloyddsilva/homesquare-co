<h1>Listing business_geopoints</h1>

<table>
  <thead>
    <tr>
      <th>Business</th>
      <th>Geopoint</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @business_geopoints.each do |business_geopoint| %>
      <tr>
        <td><%= business_geopoint.business_id %></td>
        <td><%= business_geopoint.geopoint_id %></td>
        <td><%= link_to 'Show', business_geopoint %></td>
        <td><%= link_to 'Edit', edit_business_geopoint_path(business_geopoint) %></td>
        <td><%= link_to 'Destroy', business_geopoint, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

